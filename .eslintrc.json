{
    "extends": "airbnb",
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "indent": ["error", 2],
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "react/destructuring-assignment": ["enabled", 0],
        "react/prop-types": 0,
        "max-len": 0,
        "object-shorthand": 0,
        "consistent-return": 0,
        "array-callback-return": 0
    }
}