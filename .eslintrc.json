{
    "extends": "airbnb",
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "indent": ["error", 2],
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "react/destructuring-assignment": ["enabled", "never"],
        "react/prop-types": "off" 
    }
}